    "@types/react": "^18.3.10",
    "@vixen-js/plugin-import-flat": "^0.1.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
